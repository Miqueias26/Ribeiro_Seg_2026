{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Abrir no Chrome",
            "file": "${workspaceFolder}/index.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "msedge",
            "request": "launch",
            "name": "Abrir no Edge",
            "file": "${workspaceFolder}/index.html",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Servidor Live Server",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:5500",
            "webRoot": "${workspaceFolder}"
        }
    ]
}
